    /* Responsive */
    @media (max-width: 767px) {
        .fl-table {
            display: block;
            width: 100%;
        }

        .fl-table thead, .fl-table tbody, .fl-table thead th {
            display: block;
        }

        .fl-table thead {
            float: left;
        }

        .fl-table tbody {
            width: auto;
            position: relative;
            overflow-x: auto;
        }

        .fl-table td, .fl-table th {
            padding: 20px .625em .625em .625em;
            height: 60px;
            vertical-align: middle;
            box-sizing: border-box;
            overflow-x: hidden;
            overflow-y: auto;
            width: 120px;
            font-size: 13px;
            text-overflow: ellipsis;
        }

        .fl-table thead th {
            text-align: left;
        }

        .fl-table tbody tr {
            display: table-cell;
        }

        .fl-table tbody td {
            display: block;
            text-align: center;
        }
      
    }
</style>
